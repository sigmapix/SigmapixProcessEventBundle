{
  "name": "sigmapix/process-event-bundle",
  "type": "symfony-bundle",
  "description": "Symfony SigmapixProcessEventBundle",
  "keywords": [
    "process",
    "sonata",
    "event",
    "async",
    "jobs"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Sigmapix",
      "homepage": "https://sigmapix.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "symfony/framework-bundle": "^3.4",
    "sonata-project/admin-bundle": "^3.4"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "doctrine/orm": "^2.6",
    "sonata-project/doctrine-orm-admin-bundle": "^3.6"
  },
  "suggest": {
    "sonata-project/doctrine-orm-admin-bundle": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "Sigmapix\\ProcessEventBundle\\": ""
    },
    "exclude-from-classmap": [
      "Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Sigmapix\\ProcessEventBundle\\Tests\\": "Tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  }
}
